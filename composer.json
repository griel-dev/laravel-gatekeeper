{
  "name": "mobilestock/laravel-gatekeeper",
  "description": "Laravel Gatekeeper is a package that provides a simple way to authenticate users using OAuth 2.0. Internally, it uses Laravel Socialite to authenticate users, Laravel Guards to manage the authentication process, and Laravel Routes to handle the OAuth 2.0 flow.",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "MobileStock",
      "email": "fabio@mobilestock.com.br"
    }
  ],
  "autoload": {
    "psr-4": {
      "MobileStock\\Gatekeeper\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "laravel/socialite": "^5.16",
    "illuminate/support": "^11.29",
    "illuminate/http": "^11.29",
    "illuminate/contracts": "^11.29",
    "illuminate/routing": "^11.29"
  },
  "extra": {
    "laravel": {
      "providers": [
        "MobileStock\\Gatekeeper\\Providers\\OAuthServiceProvider"
      ]
    }
  },
  "require-dev": {
    "pestphp/pest": "^3.5",
    "orchestra/testbench": "^9.5",
    "guzzlehttp/guzzle": "^7.9"
  },
  "config": {
    "allow-plugins": {
      "pestphp/pest-plugin": true
    }
  }
}
